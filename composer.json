{
  "name": "drupal/itkrate",
  "description": "ITKrate install profile",
  "type": "drupal-profile",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "~1.0",
    "drupal/token": "^1.0",
    "drupal/ctools": "^3.0",
    "drupal/pathauto": "^1.0",
    "drupal/field_group": "^1.0",
    "drupal/youtube": "^1.0",
    "drupal/entity_reference_revisions": "^1.3",
    "drupal/paragraphs": "^1.1",
    "drupal/adminimal_theme": "^1.3",
    "drupal/metatag": "^1.2",
    "drupal/role_delegation": "^1.0",
    "drupal/toolbar_visibility": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "profiles/itkore/modules/contrib/{$name}/": ["type:drupal-module"]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ]
}
